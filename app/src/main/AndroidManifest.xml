<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myactivityresult.book.com.parking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aeroview" />
        <activity android:name=".CalculateFare" />
        <activity android:name=".MyCarPosition" />
        <activity android:name=".Config" />
        <activity android:name=".NotificationNoSpace" />
        <activity android:name=".TimeLogActivity" />

        <service
            android:name=".MoneyAlarmService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".NotificationNoMoney"></activity>
    </application>

</manifest>